{
    "name": "diosaputra/repository-generator-for-laravel",
    "description": "Laravel package for generating repositories",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "repository", "generator", "pattern"],
    "authors": [
        {
            "name": "Dio Saputra",
            "email": "diosaputra20201@email.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Diosaputra\\RepositoryGenerator\\": "src/"
        }
    },
    "require": {
    "php": ">=8.0",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "illuminate/console": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Diosaputra\\RepositoryGenerator\\RepositoryGeneratorServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}